

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Date</th>
      <th>Address</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Spot</th>
      <th>Ong</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.day %></td>
        <td><%= event.address %></td>
        <td><%= event.longitude %></td>
        <td><%= event.latitude %></td>
        <td><%= event.spot %></td>
        <td><%= event.ong %></td>
        <td><%= link_to 'Show', event %></td>
        <!-- <td><%#= link_to 'Edit', edit_ong_event_path(@ong, event) %></td> -->
        <td><%= link_to 'Destroy', ong_event_path(event.ong, event), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


